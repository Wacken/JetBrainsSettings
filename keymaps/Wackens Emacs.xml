<keymap version="1" name="Wackens Emacs" parent="Emacs">
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl z" />
  </action>
  <action id=":cursive.repl.actions/jump-to-repl">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl e" />
  </action>
  <action id="Editor Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
  </action>
  <action id="EditorBackwardParagraph">
    <keyboard-shortcut first-keystroke="ctrl up" />
    <keyboard-shortcut first-keystroke="shift ctrl #10000fc" />
  </action>
  <action id="EditorForwardParagraph">
    <keyboard-shortcut first-keystroke="ctrl down" />
    <keyboard-shortcut first-keystroke="shift ctrl plus" />
  </action>
  <action id="EditorPageUp">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="ErgoKeysCommandMode">
    <keyboard-shortcut first-keystroke="alt space" />
    <keyboard-shortcut first-keystroke="context_menu" />
    <keyboard-shortcut first-keystroke="less" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="ShowPopupMenu" />
  <action id="Table-scrollUpChangeSelection">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="Tree-scrollUpChangeSelection">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
</keymap>